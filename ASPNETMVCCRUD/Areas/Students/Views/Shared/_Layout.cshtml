<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <title>@ViewData["Title"]</title>
    <meta content='width=device-width, initial-scale=1.0, shrink-to-fit=no' name='viewport' />
    <link rel="icon" href="~/lib/azzara-admin/assets/img/icon.ico" type="image/x-icon" />

    <!-- Fonts and icons -->
    <script src="~/lib/azzara-admin/assets/js/plugin/webfont/webfont.min.js"></script>
    <script>
        WebFont.load({
            google: { "families": ["Open+Sans:300,400,600,700"] },
            custom: { "families": ["Flaticon", "Font Awesome 5 Solid", "Font Awesome 5 Regular", "Font Awesome 5 Brands"], urls: ['/lib/azzara-admin/assets/css/fonts.css'] },
            active: function () {
                sessionStorage.fonts = true;
            }
        });
    </script>

    <!-- CSS Files -->
    <link rel="stylesheet" href="~/lib/azzara-admin/assets/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/lib/azzara-admin/assets/css/azzara.min.css">

    <!-- CSS Just for demo purpose, don't include it in your project -->
    <link rel="stylesheet" href="~/lib/azzara-admin/assets/css/demo.css">
    <!--   Core JS Files   -->
    <script src="~/lib/azzara-admin/assets/js/core/jquery.3.2.1.min.js"></script>
    <script src="~/lib/azzara-admin/assets/js/core/popper.min.js"></script>
    <script src="~/lib/azzara-admin/assets/js/core/bootstrap.min.js"></script>

</head>
<body>
    <div class="wrapper">
        <!--
            Tip 1: You can change the background color of the main header using: data-background-color="blue | purple | light-blue | green | orange | red"
        -->
        <partial name="_Header" />

        <partial name="_Sidebar" />

        <div class="main-panel">
            @RenderBody()
        </div>
        <partial name="_CustomSetting" />
    </div>
    </div>

    <!-- jQuery UI -->
    <script src="~/lib/azzara-admin/assets/js/plugin/jquery-ui-1.12.1.custom/jquery-ui.min.js"></script>
    <script src="~/lib/azzara-admin/assets/js/plugin/jquery-ui-touch-punch/jquery.ui.touch-punch.min.js"></script>

    <!-- jQuery Scrollbar -->
    <script src="~/lib/azzara-admin/assets/js/plugin/jquery-scrollbar/jquery.scrollbar.min.js"></script>

    <!-- Moment JS -->
    <script src="~/lib/azzara-admin/assets/js/plugin/moment/moment.min.js"></script>

    <!-- Chart JS -->
    <script src="~/lib/azzara-admin/assets/js/plugin/chart.js/chart.min.js"></script>

    <!-- jQuery Sparkline -->
    <script src="~/lib/azzara-admin/assets/js/plugin/jquery.sparkline/jquery.sparkline.min.js"></script>

    <!-- Chart Circle -->
    <script src="~/lib/azzara-admin/assets/js/plugin/chart-circle/circles.min.js"></script>

   
    <!-- Bootstrap Notify -->
    <script src="~/lib/azzara-admin/assets/js/plugin/bootstrap-notify/bootstrap-notify.min.js"></script>

    <!-- Bootstrap Toggle -->
    <script src="~/lib/azzara-admin/assets/js/plugin/bootstrap-toggle/bootstrap-toggle.min.js"></script>



    <!-- Sweet Alert -->
    <script src="~/lib/azzara-admin/assets/js/plugin/sweetalert/sweetalert.min.js"></script>

    <!-- Azzara JS -->
    <script src="~/lib/azzara-admin/assets/js/ready.min.js"></script>

    <!-- Azzara DEMO methods, don't include it in your project! -->
    <script src="~/lib/azzara-admin/assets/js/setting-demo.js"></script>
    <script src="~/lib/azzara-admin/assets/js/demo.js"></script>
</body>
</html>